{
    "name": "pandora-web",
    "env": {
        "BRANCH": "dev"
    },
    "builds": [
      {
        "src": "src/pandora/launcher.py",
        "use": "@vercel/python",
        "config": {
            "pythonVersion": "3.10"
          }
      }
    ],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "src/pandora/launcher.py"
      }
    ]
  }
  
