{
    "name": "pandora-web",
    "env": {
        "BRANCH": "alpha"
    },
    "builds": [
      {
        "src": "src/pandora/launcher.py",
        "use": "@vercel/python:3.8.12"
      }
    ],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "src/pandora/launcher.py"
      }
    ]
  }
  
